

# Redirect the first access to homepage.php
<IfModule mod_rewrite.c>
  RewriteEngine On
  # Check if the request is for the root directory
  RewriteCond %{REQUEST_URI} ^/$
  # Redirect to homepage.php
  RewriteRule ^$ homepage.php [L]
</IfModule>

